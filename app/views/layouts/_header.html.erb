<header>
  <% if current_client.nil? && current_contractor.nil? %>
    <%= render 'layouts/guest_header' %>
  <% elsif current_client.is_a?(Client) %>
    <%= render 'layouts/client_header' %>
  <% elsif current_contractor.is_a?(Contractor) %>
    <%= render 'layouts/contractor_header' %>
  <% end %>

</header>